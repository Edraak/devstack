PROGS_CFG=/app/docker-b2b.json

#Check nginx folder, ssl and include all will be mounted, one of other files will be the entry point
NGINX_ENTRY=b2b.conf

# Change mount tyoe to "-nfs" to enable nfs, otherwise leave it empty
MOUNT_TYPE=-nfs

# Enable disable services
ENABLE_PROGS=true
ENABLE_MKTG=false
ENABLE_EDX=false

ENABLE_STATE_MANAGER=false
ENABLE_JUDGE=false
ENABLE_ANALYTICS=false
ENABLE_AUTH=false
ENABLE_NOTIFIER=false
